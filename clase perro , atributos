package clase1;

/**
 *
 * @author Lab03
 */
public class Perro {
    
    
    String raza;
    String color;
    double tamaño;
    double peso;
    int edad;

public void morder() {
   
    System.out.println("el teva a morder");
    }

 public void dormir() {
     System.out.println("el perro esta dormiendo");      
    }

   public void mostrarDatos(){
        System.out.println("la raza es "+raza);
        System.out.println("el color es "+color);
        System.out.println("el tamaño es "+tamaño);
        System.out.println("el peso es "+peso);
        System.out.println("la edad  es "+edad);
        }
}








